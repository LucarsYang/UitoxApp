            <stroke android:width="0.5dip" android:color="@color/color_d5d5d5" />
            <corners android:topLeftRadius="3.0dip" android:topRightRadius="0.0dip" android:bottomLeftRadius="3.0dip" android:bottomRightRadius="0.0dip" />
