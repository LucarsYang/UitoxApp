<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#00000000" />
            <stroke android:width="0.8000001dip" android:color="#fff22e00" />
            <corners android:radius="1.0dip" />
            <padding android:top="10.0dip" android:bottom="10.0dip" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <solid android:color="#00000000" />
            <stroke android:width="0.8000001dip" android:color="#fff22e00" />
            <corners android:radius="1.0dip" />
            <padding android:top="10.0dip" android:bottom="10.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#00000000" />
            <stroke android:width="0.8000001dip" android:color="#ffdddddd" />
            <corners android:radius="1.0dip" />
            <padding android:top="10.0dip" android:bottom="10.0dip" />
        </shape>
    </item>
</selector>